{
    "name": "yusam-hub/db-ext",
    "description": "yusam-hub/db-ext",
    "type": "package",
    "minimum-stability": "stable",
    "repositories": {
        "yusam-hub/php-compatibility": {
            "type": "git",
            "url": "https://github.com/yusam-hub/php-compatibility.git"
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "yusam-hub/php-compatibility": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "YusamHub\\DbExt\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YusamHub\\DbExt\\Tests\\": "tests/"
        },
        "files": [
        ]
    }
}
